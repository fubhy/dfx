{
  "name": "dfx",
  "version": "0.5.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "description": "Effect-TS discord library",
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "pnpm run clean && pnpm run build:tsc",
    "build:tsc": "tsc -p tsconfig.json",
    "types": "discord-api-codegen ./discord-api-docs -l typescript -o 'imports=RestResponse|dfx/DiscordREST/types' 'endpointReturnType=RestResponse' > src/types.ts && prettier -w src/types.ts",
    "clean": "rm -rf dist/ *.tsbuildinfo"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "devDependencies": {
    "@tim-smart/discord-api-docs-parser": "^0.4.1",
    "@types/ws": "^8.5.3",
    "lerna": "^4.0.0",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer/compiler/typescript.tgz"
  },
  "dependencies": {
    "@effect/io": "^0.0.29",
    "@fp-ts/data": "^0.0.19",
    "callbag-effect-ts": "1.0.0-alpha.17",
    "isomorphic-ws": "^5.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "tweetnacl": "^1.0.3",
    "utf-8-validate": "^5.0.10",
    "ws": "^8.11.0"
  },
  "pnpm": {
    "overrides": {
      "@effect/io": "^0.0.28",
      "@fp-ts/data": "^0.0.19"
    }
  }
}
