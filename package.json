{
  "name": "dfx",
  "version": "0.4.0",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "description": "Effect-TS discord library",
  "author": "Tim Smart <hello@timsmart.co>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build:tsc && cp package.json dist/",
    "build:tsc": "tsc -p tsconfig.json",
    "types": "discord-api-codegen ./discord-api-docs -l typescript -o 'imports=RestResponse|dfx/DiscordRest/types' 'endpointReturnType=RestResponse' > src/types.ts && prettier -w src/types.ts",
    "clean": "rm -rf dist/"
  },
  "exports": {
    ".": "./index.js",
    "./*": "./*.js"
  },
  "devDependencies": {
    "@tim-smart/discord-api-docs-parser": "^0.4.1",
    "@types/ws": "^8.5.3",
    "dotenv": "^16.0.3",
    "lerna": "^4.0.0",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer/compiler/typescript.tgz"
  },
  "dependencies": {
    "@effect/io": "^0.0.28",
    "@fp-ts/data": "^0.0.19",
    "callbag-effect-ts": "1.0.0-alpha.11",
    "isomorphic-fetch": "^3.0.0",
    "ws": "^8.11.0"
  },
  "pnpm": {
    "overrides": {
      "@effect/io": "^0.0.28",
      "@fp-ts/data": "^0.0.19"
    }
  }
}
