{
  "name": "dfx",
  "version": "0.21.5",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "description": "Effect-TS discord library",
  "author": "Tim Smart <hello@timsmart.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tim-smart/dfx.git"
  },
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "pnpm run clean && pnpm run build:tsc && cp package.json README.md dist/",
    "build:tsc": "tsc -p tsconfig.json",
    "types": "discord-api-codegen ./discord-api-docs -l typescript -o 'imports=RestResponse|dfx/DiscordREST/types' 'endpointReturnType=RestResponse' > src/types.ts && prettier -w src/types.ts",
    "clean": "rm -rf dist/ *.tsbuildinfo"
  },
  "exports": {
    ".": "./index.js",
    "./*": "./*.js"
  },
  "devDependencies": {
    "@tim-smart/discord-api-docs-parser": "^0.5.0",
    "@types/ws": "^8.5.3",
    "lerna": "^6.1.0",
    "typescript": "https://github.com/0x706b/typescript/raw/043313ca8f/typescript-5.0.0-tsplus.20221227.tgz"
  },
  "dependencies": {
    "@effect/io": "~0.0.41",
    "@effect/stream": "~0.0.3",
    "@fp-ts/data": "~0.0.30",
    "tweetnacl": "^1.0.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "isomorphic-ws": "^5.0.0",
    "utf-8-validate": "^5.0.10",
    "ws": "^8.11.0"
  },
  "sideEffects": false,
  "gitHead": "4839b91c2459c73010d4f04e8b540d5cf2e27568"
}
