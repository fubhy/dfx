{
  "name": "dfx",
  "version": "0.3.0",
  "description": "Effect-TS discord library",
  "main": "mod.js",
  "author": "Tim Smart <tim.smart@arisechurch.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "ttsc -p tsconfig.json",
    "build:esm": "ttsc -p tsconfig.esm.json",
    "types": "discord-api-codegen ./discord-api-docs -l typescript -o 'imports=Response|./DiscordREST/types' 'endpointReturnType=Response' > types.ts && prettier -w types.ts",
    "clean": "git clean -fxd -e node_modules/ -e .env -e .vscode",
    "dev": "ts-node -C ttypescript bot.ts"
  },
  "files": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.js",
    "**/*.js.map",
    "!discord-api-docs/**/*"
  ],
  "devDependencies": {
    "@effect-ts/tracing-plugin": "^0.19.0",
    "@tim-smart/discord-api-docs-parser": "^0.3.0",
    "@types/ws": "^8.5.3",
    "callbag-effect-ts": "^0.9.3",
    "dotenv": "^16.0.0",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.7.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@effect-ts/core": "^0.59.0",
    "@effect-ts/node": "^0.38.3",
    "@effect-ts/system": "^0.56.0",
    "axios": "^0.26.1",
    "tslib": "^2.3.1",
    "ws": "^8.5.0"
  },
  "peerDependencies": {
    "callbag-effect-ts": "^0.9"
  },
  "exports": {
    ".": {
      "require": "./mod.js"
    },
    "./DiscordConfig": {
      "require": "./DiscordConfig/index.js"
    },
    "./DiscordREST": {
      "require": "./DiscordREST/index.js"
    },
    "./DiscordShard": {
      "require": "./DiscordShard/index.js"
    },
    "./DiscordWS": {
      "require": "./DiscordWS/index.js"
    },
    "./WS": {
      "require": "./WS/index.js"
    }
  }
}
